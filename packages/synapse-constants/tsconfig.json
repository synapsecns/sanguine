{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "paths": {
      "@types": [
        "./constants/types/"
      ],
      "@assets/": [
        "./constants/assets/"
      ],
      "@assets/icons/": [
        "./constants/assets/icons"
      ],
      "@assets/chains/": [
        "./constants/assets/chains"
      ],
      "@assets/*": [
        "./constants/assets/*"
      ],
    },
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}