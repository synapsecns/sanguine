{
  "extends": [
    "config:base"
  ],
  "assignees": [],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "excludePackagePrefixes": [],
      "matchManagers": ["gomod"],
      "automerge": false,
      "postUpdateOptions": [
        "gomodTidy"
      ]
    }
  ],
  "branchConcurrentLimit": 1,
  "prConcurrentLimit": 1,
  "git-submodules": {
    "enabled": true
  }
}
